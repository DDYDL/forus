<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.myHospital">
    <select id="getReservationsBetween" parameterType="map" resultType="dto.Reservation">
        SELECT *
        FROM reservation
        WHERE reserv_date BETWEEN #{start} AND #{end}
        ORDER BY reserv_date, reserv_time
    </select>



</mapper>